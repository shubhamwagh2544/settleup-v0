generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id          Int       @id @default(autoincrement())
  firstName   String    @db.VarChar(255) @map("first_name")
  lastName    String    @db.VarChar(255) @map("last_name")
}
